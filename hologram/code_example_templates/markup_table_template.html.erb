<table poop class="table">
  <tbody>
    <% examples.each do |example| %>
      <tr>
        <th>
          <div class="example-rendered">
            <h6>Source</h6>
            <%= example.rendered_example %>
          </div>
        </th>
        <td>
          <div class="example-code">
            <h6>Rendered</h6>
            <div class="highlight">
              <pre><%= example.code_example %></pre>
            </div>
          </div>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
